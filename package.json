{
  "name": "node-red-contrib-ical-events",
  "version": "1.2.6",
  "description": "NodeRed calender event adapter",
  "author": {
    "name": "naimo84",
    "email": "git@neumann-benjamin.de"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "contributors": [
    {
      "name": "naimo84",
      "email": "git@neumann-benjamin.de"
    }
  ],
  "homepage": "https://github.com/naimo84/node-red-contrib-ical-events",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/naimo84/node-red-contrib-ical-events/blob/master/LICENSE"
    }
  ],
  "standard-version": {
    "scripts": {
      "posttag": "npm pack"
    }
  },
  "keywords": [
    "node-red",
    "ical",
    "calendar",
    "caldav",
    "icloud",
    "ics",
    "Smart Home",
    "home automation"
  ],
  "node-red": {
    "nodes": {
      "ical-config": "dist/ical-config.js",
      "ical-events": "dist/ical-events.js",
      "ical-sensor": "dist/ical-sensor.js",
      "ical-upcoming": "dist/ical-upcoming.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/node-red-contrib-ical-events"
  },
  "scripts": {
    "start": "node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "release": "standard-version  --release-as minor",
    "release-bugfix": "standard-version",
    "watch": "tsc -w -p ./tsconfig.json",
    "dev": "gulp develop",
    "build": "gulp",
    "beta": "gulp && standard-version  -r major --prerelease beta"
  },
  "dependencies": {
    "cloneextend": "0.0.3",
    "cron": "^1.7.2",
    "cron-parser": "^2.13.0",
    "crypto-js": "^3.1.9-1",
    "https": "^1.0.0",
    "kalender-events": "0.15.3",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.33",
    "node-cache": "^5.1.0"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/crypto-js": "^3.1.43",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.3",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^13.13.46",
    "@types/node-red": "^1.1.1",
    "chai": "4.2.0",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "commitlint": "^8.3.5",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-watch": "^5.0.1",
    "husky": "4.0.10",
    "mocha": "7.0.0",
    "node-red": "1.2.7",
    "node-red-node-test-helper": "^0.2.7",
    "nodemon": "^1.19.4",
    "sinon": "8.1.0",
    "standard-version": "^7.1.0",
    "typescript": "^3.9.9"
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  }
}
