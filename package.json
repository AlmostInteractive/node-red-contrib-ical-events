{
  "name": "node-red-contrib-ical-events",
  "version": "0.2.1",
  "description": "NodeRed iCal Adapter",
  "author": {
    "name": "naimo84",
    "email": "git@neumann-benjamin.de"
  },
  "contributors": [
    {
      "name": "naimo84",
      "email": "git@neumann-benjamin.de"
    }
  ],
  "homepage": "https://github.com/naimo84/node-red-contrib-ical-events",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/naimo84/node-red-contrib-ical-events/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "node-red",
    "ical",
    "calendar",
    "ics",
    "Smart Home",
    "home automation"
  ],
  "node-red": {
    "nodes": {
      "ical-config": "dist/ical-config.js",
      "ical-events": "dist/ical-events.js",
      "ical-upcoming": "dist/ical-upcoming.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/node-red-contrib-ical-events"
  },
  "scripts": {
    "start": "gulp && node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "release": "gulp && standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {
    "cloneextend": "0.0.3",
    "cron": "^1.7.1",
    "cron-parser": "^2.13.0",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-ical": "^0.9.2"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "typescript": "^3.5.2",
    "commitlint": "^8.1.0",
    "@types/node": "^12.0.10",
    "@types/node-red": "^0.20.0",
    "@types/cron": "^1.7.1",
    "@types/crypto-js": "^3.1.43",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "mocha": "6.2.0",
    "node-red": "0.20.7",
    "node-red-node-test-helper": "^0.2.3",
    "standard-version": "^7.0.0",
    "should": "13.2.3",
    "husky": "3.0.3"
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
